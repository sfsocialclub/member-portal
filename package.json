    "@types/react-dom": "18.2.4",
    "@yudiel/react-qr-scanner": "2.2.2-beta.2",
    "autoprefixer": "10.4.14",
    "axios": "^1.7.9",
    "concurrently": "^8.0.1",
    "eslint": "8.40.0",
    "eslint-config-next": "13.4.2",
    "js-cookie": "^3.0.5",
    "next": "15.1.4",
    "postcss": "8.4.23",
    "qrcode.react": "^4.2.0",
    "react": "18.2.0",
    "react-day-picker": "^9.5.0",
